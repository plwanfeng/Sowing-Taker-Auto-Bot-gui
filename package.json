{
    "name": "sowing-taker-auto-bot",
    "version": "1.0.0",
    "description": "Automated farming bot for Taker's Sowing protocol with GUI",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "dev": "electron . --dev",
      "build": "electron-builder"
    },
    "keywords": [
      "taker",
      "sowing",
      "farming",
      "bot",
      "web3",
      "crypto"
    ],
    "author": "Airdrop Insiders",
    "license": "MIT",
    "dependencies": {
      "axios": "^1.6.2",
      "colors": "^1.4.0",
      "dotenv": "^16.3.1",
      "electron-log": "^5.0.0",
      "ethers": "5.7.2",
      "https-proxy-agent": "^7.0.2"
    },
    "devDependencies": {
      "electron": "^28.0.0",
      "electron-builder": "^24.6.4"
    },
    "build": {
      "appId": "com.airdropinsiders.takerbot",
      "productName": "Taker Farming Bot",
      "directories": {
        "output": "dist"
      },
      "win": {
        "target": "nsis",
        "icon": "assets/icon.ico"
      },
      "mac": {
        "target": "dmg",
        "icon": "assets/icon.icns"
      },
      "linux": {
        "target": "AppImage",
        "icon": "assets/icon.png"
      }
    }
  }